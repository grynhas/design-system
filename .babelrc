{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
        "module-resolver",
        {
            "root": ["./src"],
            "alias": {
            "@components": "./components",
            "@utils": "./utils",
            "@token": "./token",
            "@styles": "./styles"
            }
        }
        ]
    ]
}